<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="62.0" prefWidth="528.0" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.teamF.Controllers.MenuBarController">
   <children>
      <Button fx:id="mainMenuButton" mnemonicParsing="false" onAction="#mainMenuSwitch" prefHeight="33.0" prefWidth="130.0" style="-fx-background-color: #88BEC9;" text="Main Menu">
         <font>
            <Font name="HP Simplified" size="14.0" />
         </font></Button>
      <Button fx:id="displayButton" mnemonicParsing="false" onAction="#displaySwitch" prefHeight="33.0" prefWidth="139.0" style="-fx-background-color: #88BEC9;" text="Display/Load">
         <font>
            <Font name="HP Simplified" size="14.0" />
         </font></Button>
      <Button fx:id="modifyDataButton" mnemonicParsing="false" onAction="#modifyDataSwitch" prefHeight="33.0" prefWidth="134.0" style="-fx-background-color: #88BEC9;" text="Modify Data">
         <font>
            <Font name="HP Simplified" size="14.0" />
         </font></Button>
      <Button fx:id="pathFinderButton" mnemonicParsing="false" onAction="#pathfinderSwitch" prefHeight="33.0" prefWidth="115.0" style="-fx-background-color: #88BEC9;" text="Pathfinder">
         <font>
            <Font name="HP Simplified" size="14.0" />
         </font></Button>
      <Button fx:id="downloadButton" mnemonicParsing="false" onAction="#downloadSwitch" prefHeight="33.0" prefWidth="117.0" style="-fx-background-color: #88BEC9;" text="Download">
         <font>
            <Font name="HP Simplified" size="14.0" />
         </font></Button>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
