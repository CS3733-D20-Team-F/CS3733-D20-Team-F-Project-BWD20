<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.teamF.controllers.ModifyDataController">
   <children>
      <Button layoutX="234.0" layoutY="14.0" mnemonicParsing="false" onAction="#mainMenuSwitch" text="Main Menu" />
      <Button layoutX="153.0" layoutY="14.0" mnemonicParsing="false" onAction="#pathfinderSwitch" text="Pathfinder" />
      <Button layoutX="76.0" layoutY="14.0" mnemonicParsing="false" onAction="#downloadSwitch" text="Download" />
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#displaySwitch" text="Display" />
      <Button fx:id="addButton" disable="true" layoutX="169.0" layoutY="130.0" mnemonicParsing="false" onAction="#addNode" text="Add" AnchorPane.leftAnchor="169.0" />
      <Button fx:id="deleteButton" disable="true" layoutX="222.0" layoutY="130.0" mnemonicParsing="false" onAction="#deleteNode" text="Delete" AnchorPane.leftAnchor="222.0" />
      <Button fx:id="editButton" disable="true" layoutX="282.0" layoutY="130.0" mnemonicParsing="false" onAction="#editNode" text="Edit" AnchorPane.leftAnchor="282.0" />
      <TextField fx:id="nodeText" layoutX="14.0" layoutY="130.0" onKeyReleased="#validateButton" />
   </children>
</AnchorPane>
