                  </JFXButton>
               </children>
            </StackPane>
            <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1">
               <children>
                  <JFXButton onAction="#cancel" prefHeight="44.0" prefWidth="125.0" style="-fx-background-color: grey;" text="Cancel">
                     <font>
                        <Font name="STIXGeneral-Bold" size="18.0" />
                     </font>
                  </JFXButton>
               </children>
            </StackPane>
         </children>
